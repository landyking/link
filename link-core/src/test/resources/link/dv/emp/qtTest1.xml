<?xml version="1.0" encoding="UTF-8"?>
<directive xmlns="http://www.github.com/landyking/link"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.github.com/landyking/link http://www.github.com/landyking/link/link-1.0.xsd">
    <description>QueryTranslatorTest</description>
    <input>
        <param name="deptName">
            <processors>
                <QueryTranslator queryDataSource="emp" table="departments"
                                 destField="dept_no" displayField="dept_name">
                    <!--<where>dept_name not in('Production')</where>-->
                </QueryTranslator>
            </processors>
        </param>
        <param name="ld" default="223" desc="本地字典翻译测试"/>
    </input>
    <execution>
        <custom id="default" className="com.github.landyking.link.execution.custom.InputGotoOutput"/>
    </execution>
    <executionEnding/>
    <output>
        <map name="root">
            <param name="deptName" from="[exec][default].data.![#this[deptName]][0]"/>
            <param name="ld" from="[exec][default].data.![#this[deptName]][0]">
                <processors>
                    <DictTranslator dict="hello"/>
                </processors>
            </param>
        </map>
    </output>
    <resultRender/>
    <dependencys></dependencys>
</directive>