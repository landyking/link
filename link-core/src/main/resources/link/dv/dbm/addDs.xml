<?xml version="1.0" encoding="UTF-8"?>
<directive xmlns="http://www.github.com/landyking/link"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.github.com/landyking/link http://www.github.com/landyking/link/link-1.0.xsd">
    <description>新增数据源</description>
    <input>
        <param name="name" desc="名称">
            <processors>
                <Custom className="com.github.landyking.link.paramProcessor.custom.TestCustom">
                    <prop name="param2">hello 你好</prop>
                </Custom>
            </processors>
        </param>
        <param name="description" desc="描述"/>
        <param name="driverClass" desc="驱动类"/>
        <param name="jdbcUrl" desc="连接地址"/>
        <param name="username" desc="账号" notEmpty="false"/>
        <param name="password" desc="密码" notEmpty="false"/>
        <param name="extraConfig" desc="其他参数" notEmpty="false"/>
    </input>
    <execution>
        <dbInsert dataSource="default">
            <field column="name"/>
            <field column="description"/>
            <field column="driver_class" from="driverClass"/>
            <field column="jdbc_url" from="jdbcUrl"/>
            <field column="username"/>
            <field column="password"/>
            <field column="extra_config" from="extraConfig"/>
            <table>t_datasource</table>
        </dbInsert>
    </execution>
    <output>
    </output>
    <dependencys></dependencys>
</directive>